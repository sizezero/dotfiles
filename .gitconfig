[user]
	name = Robert Kleemann
	email = robert@kleemann.org
[core]
	autocrlf = input
	safecrlf = true
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[credential]
	helper = cache

## Github
[url "ssh://git@github.com/"]
     insteadOf = https://github.com/
